{
    "name": "romeoz/rock-db",
    "description": "Object Relational Mapping (ORM) for PHP.",
    "keywords": ["orm", "dbal", "active record", "database", "mysql", "postgresql", "sqlite",
      "mssql", "cubrid", "dao", "rdbms", "persistence"],
    "type": "library",
    "license": "MIT",
    "support": {
        "email": "serggalka@gmail.com",
        "issues": "https://github.com/romeOz/rock-db/issues",
        "source": "https://github.com/romeOz/rock-db"
    },
    "authors": [
        {
            "name": "Ruslan",
            "email": "serggalka@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "romeoz/rock-helpers" : "*@dev",
        "romeoz/rock-components" : "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*",
        "romeoz/rock-file" : "*@dev",
        "romeoz/rock-cache" : "*@dev",
        "romeoz/rock-validate" : "*@dev",
        "romeoz/rock-sanitize": "*@dev",
        "romeoz/rock-session": "*@dev"
    },
    "suggest": {
        "romeoz/rock-cache": "required by Rock Cache",
        "romeoz/rock-validate": "required by Rock Valiadte",
        "romeoz/rock-sanitize": "required by Rock Sanitize",
        "romeoz/rock-session": "required by Rock Session",
        "romeoz/rock-di": "required by module Rock Dependency Injection"
    },
    "autoload": {
        "psr-4": {
            "rock\\db\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9-dev"
        }
    }
}